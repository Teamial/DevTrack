{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Run DevTrack Extension",
		"type": "extensionHost",
		"request": "launch",
		"runtimeExecutable": "${execPath}",
		"args": [
		  "--extensionDevelopmentPath=${workspaceFolder}/devtrack"
		],
		"outFiles": [
		  "${workspaceFolder}/devtrack/dist/**/*.js"
		],
		"preLaunchTask": "npm: build",
		"sourceMaps": true,
		"resolveSourceMapLocations": [
		  "${workspaceFolder}/devtrack/dist/**/*.js",
		  "!**/node_modules/**"
		]
	  }
	]
  }
  